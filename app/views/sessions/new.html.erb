<div class='container'>
  <div class='wrapper'>
    <h1>Log In</h1>
    <%= form_tag sessions_path do %>
      <div>
        
        <%= text_field_tag "email", nil, placeholder: "email" %>
      </div>
      <div>
        
        <%= password_field_tag :password, nil, placeholder: "password" %>
      </div>
      <div><%= submit_tag "Log In" %></div>
    <% end %>
  </div>
</div>